{
    "compilerOptions": {
        "target": "ES2020",
        "baseUrl": "./",
        "jsx": "react",
        "outDir": "target",
        "allowJs": true,
        "checkJs": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "lib": [
            "es2017",
            "dom"
        ]
    },
    "exclude": [
        "**/dist/*",
        "**/package/*",
        "**/node_modules/*"
    ]
}